{
  "name": "new-project",
  "version": "1.0.0",
  "description": "New serverside frontend application",
  "main": "src/index.js",
  "scripts": {
    "server": "nodemon --watch server --exec babel-node --max-old-space-size=8192 --inspect server/index.js",
    "devserver": "webpack-dev-server --config webpack.clientside.dev.js",
    "build": "export NODE_ENV=production && rimraf build/* && webpack --config webpack.prod.js && babel server --out-dir build",
    "prod": "export NODE_ENV=production && node --max-old-space-size=8192 build/index.js",
    "start": "npm run build && npm run prod",
    "lint": "eslint src",
    "stylelint": "stylelint src",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|md|mdx)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run prettier -- --list-different && npm run stylelint",
    "cy:run": "cypress run",
    "cy:run:update": "yarn cy:run -- --env updateSnapshots=true",
    "cy:open": "cypress open",
    "cy:open:update": "yarn cy:open -- --env updateSnapshots=true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "osinit.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/cypress": "^4.0.5",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "config": "^3.2.2",
    "config-webpack": "^1.0.4",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.1.0",
    "cypress": "^3.4.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-cypress": "^2.6.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "^4.1.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "null-loader": "^3.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.9.3",
    "test-data-bot": "^0.8.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "express": "^4.17.1",
    "history": "^4.9.0",
    "lodash": "^4.17.19",
    "morgan": "^1.9.1",
    "mustache-express": "^1.2.8",
    "normalize.css": "^8.0.1",
    "parseurl": "^1.3.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  }
}
